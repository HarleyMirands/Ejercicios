Proceso Ejercicio6
	Dimension  K[1000]
	Para i<-1 Hasta 1000 Hacer
		k[i]= azar [100]
		si i=1
			men=k[i]
			may=k[i]
		FinSi
		si i<=500
			si k[i]<men
				men=k[i]
			FinSi
		SiNo
			si k[i]>may
				may=k[i]
			FinSi
		FinSi
	FinPara
	Imprimir "EL Menor Elemento Es", men
	Imprimir "EL Mayor Element Es", may
	FinProceso
	
